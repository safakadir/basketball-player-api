type Query {
    allPlayers: [Player]
}

type Mutation {
    addPlayer(name: String, surname: String, position: PlayerPosition): Player
    deletePlayer(id: ID): Boolean
}

type Player {
    id: ID!
    name: String
    surname: String
    position: PlayerPosition
}

enum PlayerPosition {
    PG
    SG
    SF
    PF
    C
}
